((req & G(X(!grant))) | (req & G(req) & G(X(cancel))))
((req & G(cancel)) | (req & G(X(grant))))
(F((grant & X(grant))) | F((req & G(X(!grant)))))
((cancel & X(grant)) | ((cancel) U ((req & G(X(!grant))))))
(((req & (cancel | grant))) W ((req & G(X(!grant)))))
X((((cancel | grant) & X(grant)) | (req & G(X(!grant)))))
(F((req & G(!grant))) | F(((cancel | grant) & X(grant))))
X((((G(cancel) & G(grant) & G(X(grant)))) W ((req & G(cancel)))))
(G(grant) | (req & G(!grant)) | F((req & G(cancel))))
